
include <ctime>
int main () {
const char allNums[] = "qwertyuiopasdfghjklzxcvbnmQWERTYUIOPASDFGHJKLZXCVBNM1234567890-=[]\;,./";
    int string_length = sizeof(allNums);

srand(time(0));
cout << " How many characters do you want in your random password: ";
cin >> n;
cout << " Your password is: ";
for (int i = 0; i <= n; i++){

    cout << allNums[rand() % string_length];
    return 0;
    }
