int main(){
int num = 100;
for (int i = 0; i <= num; i++){
    if (i % 3 == 0){
        cout << " Fizz " << i << endl;

    } else if (i % 5 == 0){
    cout << " Buzz " << i << endl;
    } else if (i % 5 == 0 & i % 3 == 0) {
    cout << " Fizz Buzz " << i << endl;
    } else {
    cout << i << endl;
    }
}



system ("pause>0");
 return 0;
}
